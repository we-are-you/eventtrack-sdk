{
  "name": "@wru/eventtrack",
  "version": "0.5.1",
  "description": "Event tracking SDK with ESM and CJS support",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc -p tsconfig.types.json",
    "build:js": "node build.js",
    "prepare": "npm run build",
    "dev": "npm run build && http-server . -p 3001 -c-1 --cors"
  },
  "keywords": [
    "events",
    "tracking",
    "sdk"
  ],
  "author": "Christian Linke <christian@we-are-you.de>",
  "private": false,
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/we-are-you/eventtrack-sdk"
  },
  "homepage": "https://eventtrack.dev",
  "bugs": {
    "url": "https://github.com/we-are-you/eventtrack-sdk/issues"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "prettier": "^3.4.2",
    "http-server": "^14.1.1",
    "esbuild": "^0.20.0"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  }
}
